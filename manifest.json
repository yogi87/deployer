
{
  "manifest_version": 2,
  "author": "upillay@newrelic.com",
  "name": "Click to Deploy",
  "description": "Track all the things! For more information: http://bit.ly/29aNefn",
  "version": "0.0.1",
  "icons": {"128": "icon.png" },
  "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]   
}
